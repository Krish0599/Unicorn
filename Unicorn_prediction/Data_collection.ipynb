    "path = \"data/crunchbase_pages"\n",
    "filesList = os.listdir(path)\n",
    "with open('data/startups.csv','w',newline=\"\") as file2:\n",
    "    writer = csv.writer(file2)\n",
    "    writer.writerow(['Name','Start date','Funding','Categories','Number of Articles'])\n",
    "    for fileName in filesList:\n",
    "        filePath = path + fileName\n",
    "        print(filePath)\n",
    "        startupfile = open(filePath,'rb')\n",
    "        soup = BeautifulSoup(startupfile.read(), \"html.parser\")\n",
    "        name,region = getName_Region(soup)\n",
    "        category = getSpanTagData(soup,\"Categories\")\n",
    "        date = getSpanTagData(soup,\"Founded Date\")\n",
    "        #category,date = getBasicInfo(soup)\n",
    "        date = \" \" + date\n",
    "        articles = getSpanTagData(soup,\"Number of Articles\")\n",
    "        funding = getSpanTagData(soup,\"Total Funding Amount\")\n",
    "        if funding == \"\":\n",
    "            funding = getSpanTagData(soup,\"Total Fund Raised\")\n",
    "        writer.writerow([name,date,funding,category,articles])\n",
    "        startupfile.close()\n",
