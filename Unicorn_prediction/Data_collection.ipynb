   "source": [
    "from bs4 import BeautifulSoup\n",
    "import csv\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 73,
   "metadata": {},
   "outputs": [],
   "source": [
    "filePath = 'data/AngelList.html'\n",
    "startupfile = open(filePath,'rb') \n",
    "soup = BeautifulSoup(startupfile.read(), \"html.parser\")\n",
    "div_tags = soup.findAll('div',class_=\" dc59 frw44 _a _jm\")\n",
    "startupfile.close()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 74,
   "metadata": {},
   "outputs": [],
   "source": [
    "with open('data/400StartupNames.csv','w',newline=\"\") as file1:\n",
    "    writer = csv.writer(file1)\n",
    "    for i in range(len(div_tags)-1):\n",
    "        writer.writerow([div_tags[i+1].findAll('a',class_='startup-link')[1].get_text()])\n",
    "file1.close()"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## Because crunchbase won't allow data scraping these html pages have to be manually saved in crunchbase_pages folder"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 75,
   "metadata": {},
   "outputs": [],
   "source": [
    "def getName_Region(soup):\n",
    "    div_tag = soup.find('div',class_=\"component--image-with-text-card\")\n",
    "    nameTag = div_tag.find('div',class_='ng-star-inserted')\n",
    "    regionTag = div_tag.find('a',class_='cb-link ng-star-inserted')\n",
    "    return nameTag.text,regionTag.text\n",
    "\n",
    "def getBasicInfo(soup):\n",
    "    tags = soup.find('fields-card',class_='ng-star-inserted').findAll('span',class_='field-value flex-100 flex-gt-sm-75 ng-star-inserted')\n",
    "    return tags[0].text,tags[2].text\n",
    "\n",
    "def getSpanTagData(soup,fieldName):\n",
    "    span_tags = soup.findAll(\"span\")\n",
    "    i=0\n",
    "    found = False\n",
    "    while i < len(span_tags):\n",
    "        if (span_tags[i].get_text() == fieldName):\n",
    "            #print(span_tags[i])\n",
    "            found = True\n",
    "            break\n",
    "        i+=1\n",
    "    i+=3\n",
    "    if found == True:\n",
    "        return span_tags[i].get_text()\n",
    "    else:\n",
    "        return \"\""
   ]
